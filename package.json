{
  "name": "sparkcss",
  "version": "1.3.0",
  "main": "index.js",
  "bin": {
    "sparky": "./dist/cli/cli.js"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
